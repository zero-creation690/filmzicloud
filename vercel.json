{
  "functions": {
    "api/download/[slug].py": {
      "maxDuration": 30
    },
    "api/stream/[slug].py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/download/(.*)",
      "dest": "/api/download/[slug].py",
      "methods": ["GET", "HEAD"]
    },
    {
      "src": "/api/stream/(.*)",
      "dest": "/api/stream/[slug].py",
      "methods": ["GET", "HEAD"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/webhook.py",
      "methods": ["GET", "HEAD"]
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
