{
  "functions": {
    "api/webhook.py": {
      "maxDuration": 30
    },
    "api/download/[slug].py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/download/(.*)",
      "dest": "/api/download/[slug].py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/webhook.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
